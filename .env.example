# Server API URLs
SERVER_API_URL="https://your-production-api-url.com/api/v1"
NEXT_PUBLIC_SERVER_API_URL="https://your-production-api-url.com/api/v1"

# Google OAuth credentials (replace with production values)
GOOGLE_ID="your-google-oauth-client-id"
GOOGLE_SECRET="your-google-oauth-client-secret"

# Facebook OAuth credentials (replace with production values)
FACEBOOK_CLIENT_ID="your-facebook-client-id"
FACEBOOK_CLIENT_SECRET="your-facebook-client-secret"

# Environment setting
NODE_ENV="production"

# NextAuth secret (replace with a new, secure secret)
NEXTAUTH_SECRET="your-secure-production-nextauth-secret"

# JWT configuration (expires in 1 hour, give seconds value only)
JWT_ACCESS_TOKEN_EXPIRES_IN="3600"  # 1 hour (in seconds)
